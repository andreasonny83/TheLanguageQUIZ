Options -Indexes

<IfModule mod_rewrite.c>
  RewriteEngine On
  #RewriteBase /TheLanguageQUIZ/dist/api/
  RewriteCond %{REQUEST_FILENAME} !-f
  RewriteRule ^(.*)$ v1/api.php [QSA,L]
</IfModule>

<IfModule mod_gzip.c>
  Header add X-Enabled mod_gzip
</IfModule>
<IfModule mod_deflate.c>
  Header add X-Enabled mod_deflate
</IfModule>